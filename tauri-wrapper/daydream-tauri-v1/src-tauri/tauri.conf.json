{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "daydream-tauri-v1",
  "version": "0.1.0",
  "identifier": "com.daydream.app",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "app": {
    "security": {
      "csp": null
    },
    "trayIcon": {
      "iconPath": "icons/tray-icon-Template.png",
      "iconAsTemplate": true
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Daydream",
        "width": 1200,
        "height": 800,
        "url": "https://pipelines-app-git-gio-experimenttauri-app-livepeer.vercel.app/?tauri=true",
        "label": "main"
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
